<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<Scene xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <AnchorPane prefHeight="150.0" prefWidth="500.0">
      <children>
         <VBox prefHeight="150.0" prefWidth="500.0">
            <children>
               <Label alignment="CENTER" prefHeight="44.0" prefWidth="500.0" text="Please enter your .bujo filepath to load your week or the filepath to save your week" />
               <TextField fx:id="filepathText" />
               <Label alignment="CENTER" prefHeight="58.0" prefWidth="500.0" text="Otherwise please click &quot;Continue&quot; with a blank week" />
               <HBox prefHeight="46.0" prefWidth="500.0">
                  <children>
                     <Button fx:id="loadButton" mnemonicParsing="false" prefHeight="186.0" prefWidth="238.0" text="Load" />
                     <Button fx:id="saveButton" mnemonicParsing="false" prefHeight="179.0" prefWidth="262.0" text="Save" />
                  </children>
               </HBox>
            </children>
         </VBox>
      </children></AnchorPane>
</Scene>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<Scene xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <AnchorPane prefHeight="300.0" prefWidth="500.0">
      <children>
         <VBox prefHeight="300.0" prefWidth="500.0">
            <children>
               <HBox prefHeight="25.0" prefWidth="500.0">
                  <children>
                     <Label prefHeight="25.0" prefWidth="116.0" text="Name:" />
                     <Label fx:id="miniEventName" prefHeight="25.0" prefWidth="460.0" style="-fx-border-color: #000000;" />
                  </children>
               </HBox>
               <HBox prefHeight="22.0" prefWidth="500.0">
                  <children>
                     <Label prefHeight="22.0" prefWidth="78.0" text="Day of Week:" />
                     <Label fx:id="miniEventDay" prefHeight="22.0" prefWidth="422.0" style="-fx-border-color: #000000;" />
                  </children>
               </HBox>
               <HBox prefHeight="186.0" prefWidth="500.0">
                  <children>
                     <Label prefHeight="116.0" prefWidth="78.0" text="Description:" />
                     <Label fx:id="miniEventDescription" prefHeight="189.0" prefWidth="422.0" style="-fx-border-color: #000000;" />
                  </children>
               </HBox>
               <HBox prefHeight="27.0" prefWidth="500.0">
                  <children>
                     <Label prefHeight="27.0" prefWidth="79.0" text="Start Time:" />
                     <Label fx:id="miniEventStartTime" prefHeight="27.0" prefWidth="424.0" style="-fx-border-color: #000000;" />
                  </children>
               </HBox>
               <HBox prefHeight="43.0" prefWidth="500.0">
                  <children>
                     <Label prefHeight="37.0" prefWidth="85.0" text="Duration:" />
                     <Label fx:id="miniEventDuration" prefHeight="43.0" prefWidth="373.0" style="-fx-border-color: #000000;" />
                     <Button fx:id="miniEventClose" mnemonicParsing="false" prefHeight="49.0" prefWidth="67.0" text="Close" />
                  </children>
               </HBox>
            </children>
         </VBox>
      </children></AnchorPane>
</Scene>

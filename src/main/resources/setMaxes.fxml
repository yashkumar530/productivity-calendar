<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.TextField?>
<Scene xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <AnchorPane prefHeight="200.0" prefWidth="500.0">
      <children>
         <VBox prefHeight="200.0" prefWidth="500.0">
            <children>
               <Label prefHeight="35.0" prefWidth="516.0" text="Please enter a maximum number of Tasks for each day:" />
               <TextField fx:id="maxTasksGui" />
               <Label prefHeight="43.0" prefWidth="509.0" text="Please enter a maximum number of Events for each day:" />
               <TextField fx:id="maxEventsGui" />
               <Button fx:id="setMaxButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" prefHeight="71.0" prefWidth="500.0" text="Done &amp; Save" />
            </children>
         </VBox>
      </children></AnchorPane>
</Scene>
